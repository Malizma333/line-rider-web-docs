{
  "globals": {
    "define": true,
    "Reselect": true
  },
  "env": {
    "browser": true,
    "commonjs": true, 
    "es2021": true
  },
  "overrides": [
    {
      "env": {
        "node": true
      },
      "files": [
        ".eslintrc.json"
      ],
      "parserOptions": {
        "sourceType": "script"
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-undef":"error",
    "no-unused-vars":"warn"
  }
}
